{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "linebreak-style": "off",
    "require-jsdoc": "off",
    "indent": "off",
    "operator-linebreak": "off",
    "brace-style": "off",
    "padded-blocks": "off",
    "no-multiple-empty-lines": "off"
  },
  "extends": [
//    "eslint:recommended", "google"
  ]
}
